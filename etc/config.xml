<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <forkelcustomshipping>
                <active>0</active>
                <availability>both</availability>
                <customer>0</customer>

                <scheduler_enabled>0</scheduler_enabled>
                <frequency>mon,tue,wed,thu,fri,sat,sun</frequency>
                <date_start></date_start>
                <date_end></date_end>

                <hide_enabled>0</hide_enabled>
                <hide_carriers></hide_carriers>

                <name>Custom Shipping</name>
                <title>Custom Shipping</title>
                <price>0.00</price>
                <cost>0.00</cost>
                <sallowspecific>0</sallowspecific>
                <model>Forkel\CustomShipping\Model\Carrier\Custom</model>
                <specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
            </forkelcustomshipping>
        </carriers>
    </default>
</config>